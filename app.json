{
  "expo": {
    "name": "EduApp",
    "slug": "edu-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "edu-app",
    "userInterfaceStyle": "dark",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#0C0F27"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.islamzn.eduapp"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#0C0F27"
      },
      "package": "com.islamzn.eduapp"
    },
    "web": {
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      "@react-native-firebase/app"
    ],
    "extra": {
      "androidClientId": "904016539663-2hl3h9obbbv258i4fkult09fq22pmpfn.apps.googleusercontent.com",
      "iosClientId": "904016539663-6nd311pdv4frl2qh96tf37b28fql8ome.apps.googleusercontent.com"
    }
  }
}