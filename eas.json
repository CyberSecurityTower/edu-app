{
  "cli": {
    "version": ">= 10.0.0",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal"
    },
    "preview": {
      "android": {
        "buildType": "apk"
      }
    },
    "production": {
      "autoIncrement": true,
      "env": {
        "NODE_ENV": "production"
      },
      "android": {
        "buildType": "app-bundle",
        "gradleCommand": ":app:bundleRelease -Pandroid.bundle.enableUncompressedNativeLibs=true -Pandroid.useAndroidX=true -Pandroid.enableJetifier=true"
      }
    }
  },
  "submit": {
    "production": {}
  }
}